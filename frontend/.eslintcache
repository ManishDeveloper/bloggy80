      payload:{type:'danger',msg:error.response.data.error}\r\n        })\r\n    }\r\n}\r\n\r\n\r\nexport const registerUser = (name,email,password) => async dispatch => {\r\n    try {\r\n        let body = {name,email,password}\r\n\r\n        let res = await axios.post('/api/users/register',body);\r\n\r\n        await dispatch({\r\n            type:REGISTER_SUCCESS,\r\n            payload:res.data.token\r\n        });\r\n\r\n        localStorage.setItem('token',res.data.token);\r\n\r\n        await dispatch(loadUser());\r\n\r\n        await dispatch({\r\n            type:REMOVE_ALERT\r\n        });\r\n\r\n    } catch (error) {\r\n        dispatch({\r\n            type:SET_ALERT,\r\n            payload:{type:'danger',msg:error.response.data.error}\r\n        })\r\n    }\r\n}\r\n\r\n\r\nexport const logoutUser = () => async dispatch => {\r\n\r\n    localStorage.removeItem('token');\r\n\r\n    await dispatch({type:LOGOUT});\r\n    \r\n}","F:\\tutorial\\Latest Tutorials\\React JS\\bloggy80\\frontend\\src\\redux\\reducers\\index.js",[],"F:\\tutorial\\Latest Tutorials\\React JS\\bloggy80\\frontend\\src\\components\\PageLoader.js",[],"F:\\tutorial\\Latest Tutorials\\React JS\\bloggy80\\frontend\\src\\redux\\types.js",[],"F:\\tutorial\\Latest Tutorials\\React JS\\bloggy80\\frontend\\src\\redux\\reducers\\alert.js",[],"F:\\tutorial\\Latest Tutorials\\React JS\\bloggy80\\frontend\\src\\redux\\reducers\\auth.js",[],"F:\\tutorial\\Latest Tutorials\\React JS\\bloggy80\\frontend\\src\\redux\\reducers\\post.js",[],"F:\\tutorial\\Latest Tutorials\\React JS\\bloggy80\\frontend\\src\\redux\\actions\\post.js",[],"F:\\tutorial\\Latest Tutorials\\React JS\\bloggy80\\frontend\\src\\components\\Posts.js",[],"F:\\tutorial\\Latest Tutorials\\React JS\\bloggy80\\frontend\\src\\components\\RightSidebar.js",[],"F:\\tutorial\\Latest Tutorials\\React JS\\bloggy80\\frontend\\src\\components\\UserPosts.js",["107"],"import React from 'react';\r\nimport {Col, Row, Image, Container} from 'react-boot